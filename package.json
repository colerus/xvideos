{
  "name": "xvideos-lib",
  "description": "xvideos.com api implementation and update.",
  "version": "1.1.20",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "module": "xvideos-lib",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/colerus/xvideos.git"
  },
  "bugs": {
    "url": "https://github.com/colerus/xvideos/issues"
  },
  "homepage": "https://github.com/colerus/xvideos",
  "keywords": [
    "crawler",
    "porn",
    "spider",
    "scrapper",
    "api",
    "xvideos",
    "library"
  ],
  "scripts": {
    "build": "tsc",
    "eslint": "eslint . --ext .ts",
    "test": "npm run build && cross-env NODE_ENV=test mocha --exit dist/**/*.spec.js",
    "coverage": "nyc --reporter=html --reporter=lcov npm test",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/cheerio": "0.22.23",
    "@types/mocha": "8.2.0",
    "@types/puppeteer": "5.4.2",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "codeclimate-test-reporter": "^0.5.0",
    "cross-env": "^7.0.3",
    "eslint": "7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "2.2.1",
    "tslib": "2.0.3",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": ">=7.6.0"
  }
}
