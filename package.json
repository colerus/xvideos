{
  "name": "xvideos-lib",
  "description": "xvideos.com api implementation and update.",
  "version": "1.1.13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/colerus/xvideos.git"
  },
  "bugs": {
    "url": "https://github.com/colerus/xvideos/issues"
  },
  "homepage": "https://github.com/colerus/xvideos",
  "keywords": [
    "crawler",
    "porn",
    "spider",
    "scrapper",
    "api",
    "xvideos",
    "library"
  ],
  "scripts": {
    "build": "tsc",
    "eslint": "eslint . --ext .ts",
    "test": "cross-env NODE_ENV=test mocha --exit build/lib/**/*.spec.js",
    "coverage": "nyc --reporter=html --reporter=lcov npm test",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "puppeteer": "^1.5.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/cheerio": "0.22.13",
    "@types/puppeteer": "1.20.0",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "codeclimate-test-reporter": "^0.5.0",
    "cross-env": "^5.2.0",
    "eslint": "6.5.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "1.18.2",
    "typescript": "3.6.3"
  },
  "engines": {
    "node": ">=7.6.0"
  }
}
